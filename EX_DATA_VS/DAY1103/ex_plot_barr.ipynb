    "def sangmusangmu(num):      # 상무님이 잘못 할 때마다 num +1씩 증가\n",
    "    for i in range(num):\n",
    "        print('상무', end='')\n",
    "    print('손',end='')\n",
    "    for i in range(num):\n",
    "        print('상무', end='')\n",
    "    print('님',end='')\n",
    "\n",
    "\n",
    "sangmusangmu(2)"
